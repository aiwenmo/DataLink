export type ${entity}TableListItem = {
#set($hidefield = ["isDelete","tanentId"])
#foreach($field in ${table.fields})
    #if($hidefield.indexOf(${field.propertyName})>-1)
    ${field.propertyName}?:
        #if(${field.propertyType.equals("Integer")}||${field.propertyType.equals("Double")}||${field.propertyType.equals("Float")})
            number;
        #elseif(${field.propertyType.equals("Boolean")})
            boolean;
        #elseif(${field.propertyType.equals("LocalDateTime")})
            Date;
        #else
            string;
        #end
    #else
            ${field.propertyName}: props.values.${field.propertyName},
    #end
#end
};

export type TableListPagination = {
    total: number;
    pageSize: number;
    current: number;
};

export type TableListData = {
    list: TableListItem[];
    pagination: Partial<TableListPagination>;
};

export type TableListParams = {
    status?: string;
    name?: string;
    desc?: string;
    key?: number;
    pageSize?: number;
    currentPage?: number;
    filter?: Record<string, any[]>;
    sorter?: Record<string, any>;
};
